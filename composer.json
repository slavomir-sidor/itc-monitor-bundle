{
	"name" : "sk.itcloud.symfony/itc-monitor-bundle",
	"license" : "ITCloud Symfony Monitor Bundle",
	"type" : "bundle",
	"description" : "Symfony ITC Monitor Bundle",
	"minimum-stability" : "dev",
	"autoload" : {
		"psr-0" : {
			"SK\\ITC\\MonitorBundle" : "src/"
		}
	},
	
	"bin" : [
	    "bin/monitor/cpu.sh",
		"bin/monitor/disk.sh",
		"bin/monitor/mail.sh",
		"bin/monitor/mysql.sh",
		"bin/monitor/netstat.sh",
		"bin/monitor/network.sh",
		"bin/monitor/pool.sh",
		"bin/monitor/postfix.sh",
		"bin/monitor/processes.sh",
		"bin/monitor/push.sh",
		"bin/monitor/tcpdump.sh",
		"bin/monitor.sh",
		"bin/header.sh",
		"bin/info.sh",
		"bin/socket.sh"
	],
	"config" : {
		"bin-dir" : "bin",
		"vendor-dir" : "vendor"
	},
	"require" : {
		"php" : ">=5.5.0"
	},
	"repositories" : [{
			"type" : "vcs",
			"url" : "git@github.com:slavomirkuzma/itc-monitor-bundle.git"
		}
	]
}